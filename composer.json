{
    "name": "primalmaxor/laravel-env-doctor",
    "description": "A Laravel package to diagnose and fix common environment configuration issues",
    "type": "library",
    "version": "1.0.1",
    "license": "MIT",
    "authors": [
        {
            "name": "primalmaxor",
            "email": "primalmaxor@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0",
        "vlucas/phpdotenv": "^5.0",
        "symfony/console": "^6.0|^7.0",
        "symfony/finder": "^6.0|^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0",
        "phpstan/phpstan": "^1.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Primalmaxor\\LaravelEnvDoctor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Primalmaxor\\LaravelEnvDoctor\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Primalmaxor\\LaravelEnvDoctor\\LaravelEnvDoctorServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage",
        "demo": "php examples/demo.php",
        "pint": "vendor/bin/pint",
        "stan": "vendor/bin/phpstan analyse src tests",
        "audit": "php artisan env:audit --config --detailed"
    },
    "prefer-stable": true
} 